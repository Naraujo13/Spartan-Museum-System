<AnchorPane id="LoginScreen" prefHeight="250.0" prefWidth="192.0" stylesheets="@background.css" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.102" fx:controller="ds.LoginController">
   <children>
      <Label layoutX="54.0" layoutY="14.0" text="Login" textAlignment="CENTER">
         <font>
            <Font size="34.0" />
         </font>
      </Label>
      <TextField id="CPFTextField" layoutX="22.0" layoutY="136.0" promptText="CPF" />
      <TextField id="PasswordTextField" layoutX="22.0" layoutY="171.0" promptText="Senha" />
      <Button fx:id="LoginButton" onAction="#handleLoginAction"  layoutX="22.0" layoutY="211.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="149.0" text="Logar" />
      <Label layoutX="32.0" layoutY="108.0" text="CPF ou senha incorretos" textFill="#f80505" />
   </children>
</AnchorPane>
